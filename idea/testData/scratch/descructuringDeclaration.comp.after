data class DataClass(var a: Int, val b: Int = 1, val c: Int = 2)    // RESULT: class DataClass

val (a, b, c) = DataClass(0, 1, 2)
a    // RESULT: 0
b    // RESULT: 1
c    // RESULT: 2

val (_, b1, c1) = DataClass(0, 1, 2)
b1    // RESULT: 1
c1    // RESULT: 2

val (a2) = DataClass(0)
a2    // RESULT: 0

var (a3) = DataClass(0)
a3    // RESULT: 0
a3 = 2
a3    // RESULT: 2
